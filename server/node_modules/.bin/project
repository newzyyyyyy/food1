#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/G:/QQfile/Vue3/server/node_modules/.store/project@0.1.6/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/G:/QQfile/Vue3/server/node_modules/.store/project@0.1.6/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/project@0.1.6/node_modules/project/bin/project" "$@"
else
  exec node  "$basedir/../.store/project@0.1.6/node_modules/project/bin/project" "$@"
fi
